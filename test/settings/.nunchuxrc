# Test: Global settings
# Verifies custom settings are applied

[settings]
icon_running = ▶
icon_stopped = ◯
menu_width = 70%
menu_height = 60%
popup_width = 80%
popup_height = 80%
cache_ttl = 120
primary_key = enter
secondary_key = ctrl-o
show_help = true

[app:hello]
cmd = echo "Hello world" && sleep 1
desc = Process viewer
